{
  "$schema": "https://json.schemastore.org/prettierrc",
  "tabWidth": 4,
  "printWidth": 88,
  "useTabs": false,
  "semi": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "overrides": [
    {
      "files": "*.svg",
      "options": {
        "tabWidth": 2
      }
    },
    {
      "files": "*.html",
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
